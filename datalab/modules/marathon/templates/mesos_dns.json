{
  "id": "{{ mesos_dns_id }}",
  "cmd": "sudo {{ mesos_dns_home_directory }}/mesos-dns -config={{ mesos_dns_home_directory }}/config.json",
  "instances": 1,
  "cpus": {{ mesos_dns_nb_cpus | default(0.5) }},
  "mem": {{ mesos_dns_mem }},
  "constraints": [["hostname", "UNIQUE"], ["node_type", "CLUSTER", "slave_node"]]
}
