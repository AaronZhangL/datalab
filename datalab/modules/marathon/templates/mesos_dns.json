{
  "id": "{{ mesos_dns.id }}",
  "cmd": "sudo {{ mesos_dns.home_directory }}/mesos-dns -config={{ mesos_dns.home_directory }}/config.json",
  "instances": 1,
  "cpus": {{ mesos_dns.nb_cpus }},
  "mem": {{ mesos_dns.mem }},
  "constraints": [["hostname", "UNIQUE"], ["node_dns", "CLUSTER", "True"]]
}
